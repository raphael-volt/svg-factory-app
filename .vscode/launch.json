{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Firefox Launch",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "${workspaceFolder}/dist/**"
      ],
      "pathMappings": [
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/src/"
        },
        {
          "url": "webpack:///projects/",
          "path": "${webRoot}/projects/"
        }
      ]
    },
    {
      "name": "DEBUG-ATTACH",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceFolder}",
    },
    {
      "name": "Chrome launch test [FAIL]",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Chrome launch e2e [UNKNOW]",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
      "protocol": "inspector",
      "args": [
        "${workspaceFolder}/e2e/protractor.conf.js"
      ]
    }
  ]
}