{
  "version": "0.2.0",
  "configurations": [
    {
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:4200",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
			// Uncomment this to run easily alongside another running instance of Chrome
			,"userDataDir": "${workspaceRoot}/.vscode/chrome",

			// Uncomment this to get diagnostic logs in the console
			//"trace": true
		},
		{
			"name": "Attach with sourcemaps",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
    {
      "name": "Firefox Launch",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "${workspaceFolder}/dist/**"
      ],
      "pathMappings": [
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/src/"
        },
        {
          "url": "webpack:///projects/",
          "path": "${webRoot}/projects/"
        }
      ]
    },
    {
      "name": "DEBUG-ATTACH",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceFolder}",
    },
    {
      "name": "Chrome launch test [FAIL]",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Chrome launch e2e [UNKNOW]",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
      "protocol": "inspector",
      "args": [
        "${workspaceFolder}/e2e/protractor.conf.js"
      ]
    }
  ]
}