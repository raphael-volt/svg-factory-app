{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "change-detection": [
        "dist/change-detection"
      ],
      "change-detection/*": [
        "dist/change-detection/*"
      ],
      "common": [
        "dist/common"
      ],
      "common/*": [
        "dist/common/*"
      ],
      "components": [
        "dist/components"
      ],
      "components/*": [
        "dist/components/*"
      ],
      "ng-svg": [
        "dist/ng-svg"
      ],
      "ng-svg/*": [
        "dist/ng-svg/*"
      ],
      "tinycolor": [
        "dist/tinycolor"
      ],
      "tinycolor/*": [
        "dist/tinycolor/*"
      ],
      "tspdf": [
        "dist/tspdf"
      ],
      "tspdf/*": [
        "dist/tspdf/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
