{
  "name": "svg-factory-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ngh": "ngh",
    "ng": "ng",
    "start": "ng serve --source-map=true",
    "build": "ng build",
    "test": "ng test",
    "test:d": "karma start --browsers Chrome,ChromeDebbug",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ls": "ngh ls",
    "ld": "ngh ld",
    "debug": "ng test --source-map=true",
    "build:lib": "ngh ld && ngh build",
    "build:app": "npm run ld && ng build --prod --base-href=./",
    "build:app:electron": "npm run build:app && npm run electron",
    "build:all": "npm run build:lib && npm run build:app",
    "build:web": "npm run ls && ng build --prod --source-map=false --base-href=/svg-app/",
    "build:serve": "ng build --prod --base-href=/ && npm run http-server",
    "build:watch": "ng build --prod --watch",
    "build:electron": "npm run build:all && npm run electron",
    "build:install": "rm -rf node_modules/ && warn install && npm run build:all",
    "http-server": "http-server dist/svg-factory-app/ -p 4290",
    "electron": "electron .",
    "darwin:pkg": "electron-packager . --out=release-build --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/1024X1024.icns"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngx-pwa/local-storage": "^7.2.0",
    "add": "^2.0.6",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "install": "^0.12.2",
    "maxrects-packer": "^2.0.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/pdfkit": "^0.7.36",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.0",
    "electron-packager": "^13.0.1",
    "http-server": "^0.11.1",
    "ionic": "^4.6.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons": "^3.0.1",
    "ng-packagr": "^4.2.0",
    "ng-workspace-command-helper": "^1.1.9",
    "path": "^0.12.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
